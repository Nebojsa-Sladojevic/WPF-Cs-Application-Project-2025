<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style TargetType="Path">
        <Setter Property="Data" Value="{StaticResource UndefinedIcon}"/>
        <!--Setter Property="Fill" Value="{Binding Foreground, RelativeSource={RelativeSource AncestorType=Control}}"/-->
        <!--Setter Property="Fill" Value="{StaticResource TextColor}"/-->
    </Style>
    <Style x:Key="BaseIcon" TargetType="Path" BasedOn="{StaticResource {x:Type Path}}">
        <!--Setter Property="Fill" Value="{StaticResource TextColor}"/-->
        <Setter Property="Stretch" Value="Uniform"/>
        <Setter Property="Fill" Value="{Binding Foreground, RelativeSource={RelativeSource AncestorType=Control}}"/>
    </Style>
    <Style x:Key="NavPaneIcon" TargetType="Path" BasedOn="{StaticResource BaseIcon}">
        <Setter Property="Width" Value="16"/>
        <Setter Property="Height" Value="16"/>
    </Style>
    <Viewbox x:Key="SignInLogo" Stretch="Uniform">
        <Grid>
            <Path Data="{StaticResource Logo.Leaf}" Fill="{StaticResource AccentColor}"/>
            <Path Data="{StaticResource Logo.Letters}" Fill="{StaticResource TextColor}"/>
        </Grid>
    </Viewbox>
</ResourceDictionary>