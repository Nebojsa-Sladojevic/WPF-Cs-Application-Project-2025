<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style TargetType="Path">
        <Setter Property="Data" Value="{StaticResource UndefinedIcon}"/>
    </Style>
    <Style x:Key="BaseIcon" TargetType="Path">
        <Setter Property="Stretch" Value="Uniform"/>
        <!--Setter Property="Fill" Value="{Binding Foreground, RelativeSource={RelativeSource AncestorType=Control}}"/-->
    </Style>
    <Style x:Key="NavPaneIcon" TargetType="Path" BasedOn="{StaticResource BaseIcon}">
        <Setter Property="Width" Value="16"/>
        <Setter Property="Height" Value="16"/>
        <Setter Property="Fill" Value="{StaticResource TextColor}"/>
    </Style>
</ResourceDictionary>